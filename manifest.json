{
  "name": "GPT Conversations Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension skeleton for GPT conversations.",
  "permissions": ["commands"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Toggle GPT feature"
    }
  }
}

